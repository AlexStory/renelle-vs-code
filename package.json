{
  "name": "renelle-vscode",
  "displayName": "renelle",
  "version": "0.0.3",
  "description": "Syntax highlighting for renelle",
  "repository": {
    "type": "git",
    "url": "https://github.com/alexstory/renelle-vs-code"
  },
  "publisher": "alex story",
  "engines": {
    "vscode": "^1.0.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
        "languages": [{
            "id": "renelle",
            "aliases": ["renelle", "Renelle", "rnl"],
            "extensions": [".rnl"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "renelle",
            "scopeName": "source.renelle",
            "path": "./syntaxes/renelle.tmLanguage.json"
        }]
    },
  "scripts": {
    "build": "vsce package"
  },
  "author": "alex story",
  "license": "MIT",
  "devDependencies": {
    "vscode": "^1.1.37"
  }
}
